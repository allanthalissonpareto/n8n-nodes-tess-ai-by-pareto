{
  "name": "n8n-nodes-tess-ai-by-pareto",
  "version": "1.0.8",
  "description": "Community node for integrating Tess AI with n8n",
  "author": {
    "name": "Pareto",
    "email": "allan.thalisson@pareto.plus"
  },
  "license": "MIT",
  "keywords": [
    "n8n",
    "workflow",
    "automation",
    "tess",
    "ai",
    "node",
    "n8n-community-node-package"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pareto-io/n8n-nodes-tess-ai-by-pareto"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "./dist/credentials/tessApi.credentials.js"
    ],
    "nodes": [
      "./dist/nodes/Agents/agents.node.js",
      "./dist/nodes/Files/files.node.js",
      "./dist/nodes/Webhooks/webhooks.node.js"
    ]
  },
  "scripts": {
    "build": "rimraf dist && tsc && gulp",
    "lint": "eslint ./nodes --ext .ts",
    "format": "prettier --write \"**/*.{js,ts,json}\""
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n8n-nodes-base": "^1.4.1",
    "n8n-workflow": "^1.82.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "gulp": "^4.0.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}